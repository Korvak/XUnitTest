@model BuyOrderResponse
<h2 style="margin-left: 20px">
    <i class="fa-solid fa-money-bill-trend-up"></i>
    Stocks
</h2>
<div class="box">
    <p class="text-blue">@Model.StockName (@Model.StockSymbol)</p>
    <p style="color:black;font-size:1.75em;font-weight:600;">
        <i class="fa-solid fa-dollar-sign text-grey" style="font-size:0.8em;"></i>
        @Model.Price   
    </p>
</div>